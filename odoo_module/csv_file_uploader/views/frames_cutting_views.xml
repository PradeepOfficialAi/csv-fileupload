<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_frames_cutting_record_list" model="ir.ui.view">
        <field name="name">frames.cutting.record.list</field>
        <field name="model">frames.cutting.record</field>
        <field name="arch" type="xml">
            <list string="Frame Cutting">
                <field name="col_a"/>
                <field name="col_b"/>
                <field name="col_c"/>
                <field name="col_d"/>
                <field name="col_e"/>
                <field name="col_f"/>
                <field name="col_g"/>
                <field name="col_h"/>
            </list>
        </field>
    </record>

