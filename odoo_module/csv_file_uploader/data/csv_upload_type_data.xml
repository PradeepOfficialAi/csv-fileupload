<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="csv_upload_type_workorder" model="csv.upload.type">
        <field name="name">Work Orders</field>
        <field name="code">WORKORDER</field>
        <field name="target_model">work.order.record</field>
        <field name="primary_key">order_number</field>
        <field name="secondary_key">line_reference</field>
        <field name="description">Desktop CSV profile for standard work orders.</field>
    </record>

    <record id="csv_upload_type_workorder_alt" model="csv.upload.type">
        <field name="name">Work Orders 2</field>
        <field name="code">WORKORDER2</field>
        <field name="target_model">work.order.alternate.record</field>
        <field name="primary_key">order_number</field>
        <field name="secondary_key">line_reference</field>
        <field name="description">Alternate work order layout.</field>
    </record>

    <record id="csv_upload_type_frames_cutting" model="csv.upload.type">
        <field name="name">Frame Cutting</field>
        <field name="code">FRAMESCUTTING</field>
        <field name="target_model">frames.cutting.record</field>
        <field name="description">Frame cutting CSV generated by desktop client.</field>
    </record>

    <record id="csv_upload_type_casing_cutting" model="csv.upload.type">
        <field name="name">Casing Cutting</field>
        <field name="code">CASINGCUTTING</field>
        <field name="target_model">casing.cutting.record</field>
        <field name="primary_key">order_ref</field>
        <field name="description">Casing cutting CSV generated by desktop client.</field>
        <field name="field_schema"><![CDATA[{
            "H_W": {"field": "hw"},
            "BIN": {"field": "bin_code"},
            "ORDER_LINE": {"field": "order_line"},
            "MATERIAL": {"field": "material"},
            "LABEL": {"field": "label"},
            "ORDER": {"field": "order_ref"},
            "WINDOW": {"field": "window_name"},
            "WINDOWS_SIZE": {"field": "windows_size"},
            "ROSSETTE": {"field": "rossette"},
            "CASING LINE": {"field": "casing_line"},
            "COMPANY": {"field": "company"},
            "PO": {"field": "po"},
            "DATE": {"field": "order_date"},
            "TIME": {"field": "order_time"},
            "USER": {"field": "user_name"}
        }]]></field>
    </record>

    <record id="csv_upload_type_glass_report" model="csv.upload.type">
        <field name="name">Glass Reports</field>
        <field name="code">GLASSREPORT</field>
        <field name="target_model">glass.report.record</field>
        <field name="primary_key">order_ref</field>
        <field name="secondary_key">sealed_unit_id</field>
        <field name="description">Glass report statistics.</field>
    </record>

    <record id="csv_upload_type_invoice_dates" model="csv.upload.type">
        <field name="name">Invoice Dates</field>
        <field name="code">INVOICEDATES</field>
        <field name="target_model">invoice.date.record</field>
        <field name="primary_key">order_number</field>
        <field name="secondary_key">invoice_number</field>
        <field name="description">Invoice dates exported from legacy system.</field>
    </record>

    <record id="csv_upload_type_window_entry" model="csv.upload.type">
        <field name="name">Window Entries</field>
        <field name="code">WINDOWSENTRY</field>
        <field name="target_model">window.entry.record</field>
        <field name="primary_key">order_number</field>
        <field name="secondary_key">quotation_number</field>
        <field name="description">Window entry counts for production.</field>
    </record>

    <record id="csv_upload_type_quotation_to_order" model="csv.upload.type">
        <field name="name">Quotation To Order</field>
        <field name="code">QUOTAIONTOORDER</field>
        <field name="target_model">quotation.to.order.record</field>
        <field name="primary_key">quotation_number</field>
        <field name="secondary_key">to_order_number</field>
        <field name="description">Quotation-to-order conversion metrics.</field>
    </record>

    <record id="csv_upload_type_quotation_to_order_alias" model="csv.upload.type">
        <field name="name">Quotation To Order (Alias)</field>
        <field name="code">QUOTATIONTOORDER</field>
        <field name="target_model">quotation.to.order.record</field>
        <field name="primary_key">quotation_number</field>
        <field name="secondary_key">to_order_number</field>
        <field name="description">Alternative code spelling for quotation-to-order files.</field>
    </record>
</odoo>
